{
  "name": "tana",
  "version": "1.0.0",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "npm run build:main && concurrently \"npm run dev:vite\" \"npm run dev:main\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "forge:make": "npm run build && electron-forge make"
  },
  "keywords": [],
  "author": "cxOrz",
  "license": "ISC",
  "description": "Tana is a desktop pet",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vue-tsc": "^3.1.0",
    "wait-on": "^9.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "pixi.js": "^8.14.0",
    "vue": "^3.5.22"
  }
}
