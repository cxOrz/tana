{
  "name": "tana",
  "version": "1.0.0",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "npm run build:main && concurrently \"npm run dev:vite\" \"npm run dev:main\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:main": "tsc -p src/main/tsconfig.json --watch",
    "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p src/main/tsconfig.json",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "forge:make": "electron-forge make",
    "make": "npm run build && npm run forge:make",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "cxOrz",
  "license": "ISC",
  "description": "Tana is a desktop pet",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^39.2.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.1.7",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0",
    "wait-on": "^9.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.28",
    "@tailwindcss/vite": "^4.1.16",
    "ai": "^5.0.108",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-vue-next": "^0.548.0",
    "pixi.js": "^8.14.0",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "zod": "^4.1.13"
  }
}
